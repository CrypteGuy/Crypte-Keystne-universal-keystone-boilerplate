{
  "name": "universal-keystone",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "async": "1.5.0",
    "babel-core": "6.3.17",
    "babel-plugin-object-assign": "1.2.1",
    "dotenv": "1.2.0",
    "express": "4.13.3",
    "keystone": "ericelliott/keystone",
    "react": "0.14.3",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "babel-cli": "6.3.17",
    "babel-core": "6.3.17",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "babel-plugin-react-transform": "1.1.1",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.11.3",
    "supertest": "1.1.0",
    "tape": "4.2.2",
    "updtr": "0.1.6",
    "watch": "0.16.0",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "start:db": "mongod --dbpath data/db",
    "start": "NODE_PATH=source babel-node source/keystone.js",
    "test": "NODE_PATH=source babel-node source/test/index.js",
    "test:routes": "NODE_PATH=source babel-node source/test/functional/routes/index.js",
    "watch": "watch 'clear && npm run -s lint && npm run -s test' source",
    "watch:routes": "watch 'clear && npm run -s lint && npm run -s test:routes' source",
    "precheck": "npm run -s lint",
    "check": "npm run -s test && npm run -s test:routes",
    "latest": "updtr"
  },
  "main": "keystone.js"
}
